{
  "compilerOptions": {                 
    "target": "es6",                               
    "module": "commonjs",                           
    "lib": ["es2019"],                              
    "outDir": "./dist",                              
    "rootDir": "src",                              
    "strict": true,                                 
    "noImplicitAny": true,                          
    "esModuleInterop": true,                     
    "skipLibCheck": true,                           
    "forceConsistentCasingInFileNames": true        
  }
}
